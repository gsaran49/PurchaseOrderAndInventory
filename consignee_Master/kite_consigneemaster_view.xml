					<group>			
						<group>			
							<field name="Name"/>
							<field name="Email"></field>	
							<field name="Phone_No"/>
						</group>	
							   
						<group>
							<field name="Website"/>
							<field name="state"/>
						</group>
					</group>
					<!--
					<field name = "line_id"/>
					-->
				</form>
			</field> 
		</record>
		
		<record id="kite_consigneemaster_tree_view_1" model="ir.ui.view">
			<field name="name">kite.consigneemaster.tree</field>
			<field name="model">kite.consigneemaster</field>
			<field name="arch" type="xml">
				<tree toolbar="True" string="Purchase">
					<field name="Name"/>
					<field name="Phone_No"/>
<!--
					<field name="status"/>
-->
				</tree>
			</field>
		</record>
		

	
		
