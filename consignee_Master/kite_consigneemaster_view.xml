	<record id="kite_consigneemaster_form_view_1" model="ir.ui.view">
			<field name="name">kite.consigneemaster.form</field>
			<field name="model">kite.consigneemaster</field>
			<field name="arch" type="xml">
				<form string="kite consigneemaster" version="7.0">	
					<header>
